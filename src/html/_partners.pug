-var section = $i18n.pages.partners
h2.desktop-align-center-1024.margin-bottom-24.color-blue200.font-weight-bold= section.header
div.partners-img--container
    each partner in section.partners
        div.partner--container
            a(href=partner.link)
                div(class=partner.class)
                    img.img-fluid(src=partner.image alt=partner.image)